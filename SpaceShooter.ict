    {
      "name": "PlayerShip",
      "depth": 0,
      "oncreate": "this.gameOver = false;",
      "onstep": "if (ct.keyboard.down['left']) {\n    this.x -= 4 * ct.delta;\n}\nif (ct.keyboard.down['right']) {\n    this.x += 4 * ct.delta;\n}\n\nif (ct.keyboard.down['up']) {\n    this.y -= 4 * ct.delta;\n}\nif (ct.keyboard.down['down']) {\n    this.y += 4 * ct.delta;\n}\n\nif (this.x < 0) {\n    this.x = 0;\n}\nif (this.x > ct.width) {\n    this.x = ct.width;\n}\n\nif (this.y < 0) {\n    this.y = 0;\n}\nif (this.y > ct.height) {\n    this.y = ct.height;\n}\n\nif (ct.keyboard.pressed['space']) {\n    ct.types.copy('BlueLaser', this.x, this.y);\n}\n\nthis.move();\n\nvar hostile = ct.place.occupied(this, this.x, this.y, 'Hostile');\nif (hostile) {\n    hostile.kill = true;\n    ct.room.lives--;\n    if (ct.room.lives <= 0) {\n        this.kill = true;\n        setTimeout(function() {\n           ct.rooms.switch('room1');\n        }, 500);\n    }\n}",
