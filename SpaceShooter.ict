      "oncreate": "this.asteroidTimer = 20;\nthis.enemyTimer = 180;\nthis.score = 0;\nthis.lives = 3;\n\nthis.scoreLabel = new PIXI.Text('Score: ' + this.score, ct.styles.get('ScoreText'));\nthis.addChild(this.scoreLabel);\nthis.scoreLabel.x = 30;\nthis.scoreLabel.y = 30;\n\nthis.livesLabel = new PIXI.Text('Lives: ' + this.score, ct.styles.get('ScoreText'));\nthis.addChild(this.livesLabel);\nthis.livesLabel.x = ct.width - 200;\nthis.livesLabel.y = 30;",
      "onstep": "this.asteroidTimer -= ct.delta;\nif (this.asteroidTimer <= 0) {\n    this.asteroidTimer = ct.random.range(20, 200);\n    ct.types.copy(ct.random.dice('BigMeteor', 'MidMeteor'), ct.random(ct.width), -100);\n}\n\nthis.enemyTimer -= ct.delta;\nif (this.enemyTimer <= 0) {\n    this.enemyTimer = ct.random.range(180, 400);\n    ct.types.copy('EnemyShip', ct.random(ct.width), -100);\n}\n\n/*if (ct.types.PlayerShip.gameOver) {\n    ct.rooms.switch('Main');\n}*/",
